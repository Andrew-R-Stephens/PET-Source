<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape>
            <solid
                android:color="?attr/backgroundColor"
                />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="4dp"
                />
            <stroke
                android:color="@color/white_M100"
                android:width="2dp" />
            <size
                android:height="16dp"
                android:width="16dp" />
        </shape>

    </item>

    <item
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp"
        android:left="2dp"
        >
        <bitmap
            android:src="@drawable/icon_inconclusive_unselected"
            />
    </item>

</layer-list>