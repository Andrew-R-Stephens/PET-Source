<?xml version="1.0" encoding="utf-8"?>

<level-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/tier1_icon"
        android:minLevel="1"
        android:maxLevel="1" >
        <bitmap
            android:src="@drawable/icon_tier_1"
            android:tint="?attr/codex_3"
            android:tintMode="multiply">
        </bitmap>
    </item>
    <item
        android:id="@+id/tier2_icon"
        android:minLevel="2"
        android:maxLevel="2" >
        <bitmap
            android:src="@drawable/icon_tier_2"
            android:tint="?attr/codex_3"
            android:tintMode="multiply">
        </bitmap>
    </item>

    <item
        android:id="@+id/tier3_icon"
        android:minLevel="3"
        android:maxLevel="3" >
        <bitmap
            android:src="@drawable/icon_tier_3"
            android:tint="?attr/codex_3"
            android:tintMode="multiply">
        </bitmap>
    </item>
</level-list>
