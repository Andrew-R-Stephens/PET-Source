<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval">

            <size
                android:width="200dp"
                android:height="200dp"/>
            <stroke
                android:color="@color/white"
                android:width="1dp"
                />
        </shape>
    </item>
    <item
        android:id="@+id/sanity_bottom">
        <bitmap
            android:src="@drawable/icon_sanityhead_bottom" />
    </item>
    <item
        android:id="@+id/sanity_top">
        <bitmap
            android:src="@drawable/icon_sanityhead_top" />
    </item>

</layer-list>