<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid
                android:color="?attr/backgroundColorOnBackground"/>
            <corners
                android:radius="32dp" />

        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:color="@color/white"
                android:width="3dp"
                />
            <corners
                android:radius="30dp" />
            <padding
                android:left="20dp"
                android:top="20dp"
                android:right="20dp"
                android:bottom="20dp" />
        </shape>
    </item>

</layer-list>
