<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">

        <item
            android:minLevel="0"
            android:maxLevel="0" >
            <shape>
                <solid
                    android:color="@color/transparent"/>
            </shape>
        </item>
        <item
            android:minLevel="1"
            android:maxLevel="1"
            android:drawable="@drawable/ic_arrow_60_left" />
        <item
            android:minLevel="2"
            android:maxLevel="2"
            android:drawable="@drawable/ic_arrow_60_right" />
        <item
            android:minLevel="3"
            android:maxLevel="3"
            android:drawable="@drawable/ic_selector_pos_unsel"/>
        <item
            android:minLevel="4"
            android:maxLevel="4"
            android:drawable="@drawable/ic_selector_neg_unsel"/>
        <item
            android:minLevel="5"
            android:maxLevel="5"
            android:drawable="@drawable/ic_control_play" />
        <item
            android:minLevel="6"
            android:maxLevel="6"
            android:drawable="@drawable/ic_control_pause" />

</level-list>