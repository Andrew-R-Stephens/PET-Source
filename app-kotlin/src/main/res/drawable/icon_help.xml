<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <size
                android:width="48dp"
                android:height="48dp" />
            <solid
                android:color="?attr/backgroundColor"
                />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="4dp"
                />
            <stroke
                android:color="@color/white_M100"
                android:width="2dp" />
        </shape>

    </item>

    <item
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:drawable="@drawable/icon_inconclusive_unselected" />

</layer-list>