<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fragmentnavgraph"
    app:startDestination="@id/evidenceFragment">

    <fragment
        android:id="@+id/evidenceFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.evidence.children.solo.EvidenceSoloFragment"
        android:label="EvidenceFragment">
        <action
            android:id="@+id/action_evidence_to_objectives"
            app:destination="@id/objectivesFragment" />
        <action
            android:id="@+id/action_evidence_to_mapmenu"
            app:destination="@id/mapMenuFragment" />
        <action
            android:id="@+id/action_evidenceFragment_to_utilitiesFragment"
            app:destination="@id/utilitiesFragment" />
    </fragment>
    <fragment
        android:id="@+id/objectivesFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.missions.MissionsFragment"
        android:label="ObjectivesFragment" >
        <action
            android:id="@+id/action_objectives_to_evidence"
            app:destination="@id/evidenceFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapMenuFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.mapsmenu.MapMenuFragment"
        android:label="MapMenuFragment" >
        <action
            android:id="@+id/action_mapmenu_to_evidence"
            app:destination="@id/evidenceFragment" />
        <action
            android:id="@+id/action_mapmenu_to_mapview"
            app:destination="@id/mapViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapViewFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.mapsmenu.mapdisplay.MapViewerFragment"
        android:label="MapViewFragment" >
        <action
            android:id="@+id/action_mapViewFragment_to_evidenceFragment"
            app:destination="@id/evidenceFragment" />
        <action
            android:id="@+id/action_mapview_to_mapmenu"
            app:destination="@id/mapMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/utilitiesFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.utilities.UtilitiesFragment"
        android:label="UtilitiesFragment" >
        <action
            android:id="@+id/action_utilitiesFragment_to_evidenceFragment"
            app:destination="@id/evidenceFragment" />
        <action
            android:id="@+id/action_utilitiesFragment_to_petHelpFragment"
            app:destination="@id/petHelpFragment" />
        <action
            android:id="@+id/action_utilitiesFragment_to_highscoresFragment"
            app:destination="@id/highscoresFragment" />
        <action
            android:id="@+id/action_utilitiesFragment_to_codexFragment"
            app:destination="@id/codexFragment" />
        <action
            android:id="@+id/action_utilitiesFragment_to_ghostBoxFragment"
            app:destination="@id/ghostBoxFragment" />
    </fragment>
    <fragment
        android:id="@+id/highscoresFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.utilities.HighscoresFragment"
        android:label="HighscoresFragment" >
        <action
            android:id="@+id/action_highscoresFragment_to_utilitiesFragment"
            app:destination="@id/utilitiesFragment" />
    </fragment>
    <fragment
        android:id="@+id/petHelpFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.utilities.PetHelpFragment"
        android:label="PetHelpFragment" >
        <action
            android:id="@+id/action_petHelpFragment_to_utilitiesFragment"
            app:destination="@id/utilitiesFragment" />
    </fragment>
    <fragment
        android:id="@+id/codexFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.utilities.CodexFragment"
        android:label="CodexFragment" >
        <action
            android:id="@+id/action_codexFragment_to_utilitiesFragment"
            app:destination="@id/utilitiesFragment" />
    </fragment>
    <fragment
        android:id="@+id/ghostBoxFragment"
        android:name="com.TritiumGaming.phasmophobiaevidencepicker.activities.fragments.investigation.utilities.ghostboxutility.GhostBoxFragment"
        android:label="GhostBoxFragment" >
        <action
            android:id="@+id/action_ghostBoxFragment_to_utilitiesFragment"
            app:destination="@id/utilitiesFragment" />
    </fragment>
</navigation>