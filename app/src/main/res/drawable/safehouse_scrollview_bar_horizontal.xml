<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape
            android:padding="10dp"
            android:shape="rectangle">
            <solid
                android:color="?attr/selectedColor"
                />
            <size
                android:height="10dp"
                android:width="20dp" />
            <corners
                android:radius="2dp"/>
        </shape>
    </item>

</layer-list>